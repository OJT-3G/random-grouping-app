{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check --ignore-unknown \"**/*\"",
